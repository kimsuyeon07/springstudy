package ex03_xml;

import java.util.List;

public class ListBean {

	
	// field (property)
	private List<String> list;
	
	// constructor
	
	// method
	public void info() {
		for(int i = 0, length= list.size(); i < length; i++) {
			System.out.println((i + 1) + "번째요소: " + list.get(i));
		}
	}
	
	
	
	
	
	
	
}
