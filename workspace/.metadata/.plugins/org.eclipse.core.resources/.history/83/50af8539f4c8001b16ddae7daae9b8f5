package ex03_xml;

import java.util.HashMap;
import java.util.Map;

public class MapBean {
	
	// field (property)
	private Map<String, String> map;
	
	// constructor
	
	// method
	public void info() {
		for (Map.Entry<String, String> entry : map.entrySet()) {
			System.out.println(entry.getKey() + " : " + entry.getValue());
		}
		
	}
	

}
