package ex04_xml;

public class Calculator {

	// field (property)
	// consturctor
	
	// method
	public int add(int a, int b) { return a + b; }
	public int substract(int a, int b) { return a + b; }
	public int multiply(int a, int b) { return a + b; }
	public int divide(int a, int b) { return a + b; }
	
	
}
