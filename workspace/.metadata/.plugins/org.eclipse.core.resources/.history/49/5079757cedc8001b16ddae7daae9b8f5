	<!--  
		지금부터 아래 Bean을 생성한다.
		List<String> list = new ArrayList<>();
		list.add("튀김");
		list.add("떡볶이");
		list.add("순대");
		ListBean listBean = new ListBean();
		listBean.setList(list);
	-->

	<bean id="listBean" class="ex03_xml.ListBean">
		<property name="list">
			<list>
				
			</list>
		</property>
	</bean>



</beans>
