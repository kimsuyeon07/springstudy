package ex07_java_into_xml;

public class Publisher {

	
	// field (property)
	private String name;
	private String location;
	
	
	// constructor
	public Publisher() {}

	public Publisher(String name, String location) {
		super();
		this.name = name;
		this.location = location;
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
}
