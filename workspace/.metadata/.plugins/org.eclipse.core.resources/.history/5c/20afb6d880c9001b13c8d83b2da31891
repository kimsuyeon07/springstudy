package ex04_xml;

public class BMICalculator {

	// method

	// field (property)
	// private double low;	// 저체중 (low < 20), normal 기준으로 처리하므로 불필요하다. (굳이 저장하지 않아도 됨) 
	private double normal;  // 정상(20 <= normal < 25) , 20 저장
	private double over; 	// 과체중 (25 <= over < 30), 25 저장
	private double obesity; // 비만 (30 <= obesity), 30 저장 
	
	
	// constructor
	public BMICalculator() {}

	public BMICalculator(double normal, double over, double obesity) {
		super();
		this.normal = normal;
		this.over = over;
		this.obesity = obesity;
	}

	
	
	// Getter and Setter
	public double getNormal() {
		return normal;
	}

	public void setNormal(double normal) {
		this.normal = normal;
	}

	public double getOver() {
		return over;
	}

	public void setOver(double over) {
		this.over = over;
	}

	public double getObesity() {
		return obesity;
	}

	public void setObesity(double obesity) {
		this.obesity = obesity;
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
